// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model pdbCollection {
  id        Int    @id @default(autoincrement())
  randomStr String @unique @default(dbgenerated("(hex(randomblob(5)) || hex(randomblob(5)))"))
  pdb       String
}

model user {
  id        Int    @id @default(autoincrement())
  username  String
  color     String
  selection String
  randSt    String
}
